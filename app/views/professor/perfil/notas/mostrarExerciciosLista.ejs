<% include ../../../partials/professor-top %>
<div class="card-body">

  <h3> Respostas </h3>

  <hr>

  <% for( let i = 0; i < respostas.length; i++ ){ %>

  <div class="col-md-10">
    <div class="card row">
      <div class="card-header">
        <h4 class="card-title">
          <%= respostas[i].titulo%>
        </h4>
      </div>
      <div class="card-body">
        <p class="card-text">
          <%= respostas[i].resposta%>
        </p>
      </div>
    </div>
  </div>

  <br>

  <% } %>

</div>
<% include ../../../partials/professor-bottom %>