  <hr>

  <div class="col-md-10">
    <div class="card">

      <div class="card-header">
        <h5>
          <p>
            <%= didatico[0].titulo%>
          </p>
        </h5>
      </div>

      <div class="card-body">
        <h5>
          <p>
            <%= didatico[0].descricao%>
          </p>
        </h5>
      </div>

      <div class="card-body">

        <% for (let i = 0; i < paths.length; i++ ) { %>
        <a href="/turmas/didaticos/abrir/<%= didatico[0].id%>/download/<%= paths[i].file_name%>" class="list-group-item list-group-item-action">
          <div class="row">
            <div class="col-md-1"><i class="fa fa-download" aria-hidden="true"></i></div>
            <div class="col-md-10">
              <p>
                <%= paths[i].file_name %>
              </p>
            </div>
          </div>
        </a>
        <% } %>
      </div>

      <br>
    </div>
  </div>
  <!-- end content -->
</div>
<% include ../../../partials/aluno-bottom %>