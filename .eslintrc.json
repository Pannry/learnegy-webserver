{
    "extends": "airbnb-base",
    "rules": {
        "no-unused-expressions": [
            "error",
            {
                "allowTernary": true
            }
        ],
        "no-underscore-dangle": [
            "error",
            {
                "allow": [
                    "_conexaoDb"
                ]
            }
        ],
        "func-names": [
            "error",
            "never"
        ],
        "linebreak-style": [
            "error",
            "windows"
        ]
    }
}