{
  "name": "projel-webserver",
  "version": "1.0.0",
  "description": "web server for projel project",
  "main": "index.js",
  "scripts": {
    "devdb": "nodemon --inspect server.js",
    "dev": "nodemon server.js",
    "lint": "node node_modules/ESLint/bin/eslint app/**/*.js",
    "m:reset": "db-migrate db:drop projel -e reset && db-migrate db:create projel -e reset",
    "m:setup": "npm run m:reset && db-migrate up",
    "m:resetprod": "db-migrate db:drop projel -e prod && db-migrate db:create projel -e prodsetup",
    "m:prod": "npm run m:resetprod && db-migrate up -e prod",
    "start": "node server.js"
  },
  "engines": {
    "node": "10.17.0",
    "npm": "6.11.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pannry/projel-webserver.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Pannry/projel-webserver/issues"
  },
  "homepage": "https://github.com/Pannry/projel-webserver#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "connect-flash": "^0.1.1",
    "consign": "^0.1.6",
    "cookie-parser": "^1.4.3",
    "db-migrate": "^0.11.6",
    "db-migrate-mysql": "^2.1.0",
    "dotenv": "^6.2.0",
    "ejs": "^2.6.1",
    "express": "^4.17.1",
    "express-session": "^1.16.1",
    "multer": "^1.3.0",
    "mysql2": "^2.0.2",
    "passport": "^0.2.2",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.17.3"
  }
}